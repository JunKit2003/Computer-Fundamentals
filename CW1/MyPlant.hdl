CHIP MyPlant{
IN a,b,c;
OUT out;

PARTS:
Not(in=a, out=xNot);
Not(in=c, out=zNot);

And(a=xNot, b=c, out=Aand);
And(a=b, b=zNot, out=Band);

Or(a=Aand, b=Band, out=out);

}

